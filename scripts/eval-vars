#!/bin/bash
VARS=$(env | grep "${1}")
if [ -z "$VARS" ]; then
  exit 0;
fi

function token_quote {
  local quoted=()
  for token; do
   quoted+=( "$(printf '%q' "$token")" )
  done
  printf '%s\n' "${quoted[*]}"
}

while IFS='=' read -r key value; do
  export "${key}"="$(eval token_quote "$value")"
  echo "Processed variable: ${key}"
done <<< "$VARS"

